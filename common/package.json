{
  "name": "@zotijstestingms/common",
  "version": "1.0.0",
  "description": "A library with the common code of the packages",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/common.umd.cjs",
  "module": "./dist/common.js",
  "types": "./dist/common.d.ts",
  "exports": {
    ".": {
      "import": "./dist/common.js",
      "require": "./dist/common.umd.cjs",
      "types": "./dist/common.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts --report-unused-disable-directives",
    "typechecks": "tsc --noEmit",
    "test": "vitest",
    "release": "changesets publish"
  },
  "keywords": [
    "vite",
    "fastify"
  ],
  "author": "zotijs",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "fastify": "^4.25.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.1"
  },
  "dependencies": {
    "fastify-plugin": "^4.5.1"
  },
  "peerDependencies": {
    "fastify": "^4.25.2"
  }
}
