    "eslint": "^9.8.0",
    "fastify": "^4.28.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.3.5",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vitest": "^2.0.4"
