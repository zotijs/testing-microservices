{
  "name": "assets",
  "version": "1.0.0",
  "description": "Static assets service",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch ./index.ts",
    "build": "vite build && cp -R ./static ./dist",
    "start": "node dist/index.cjs",
    "lint": "eslint .",
    "typechecks": "tsc --noEmit",
