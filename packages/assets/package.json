  },
  "keywords": [
    "vite",
    "fastify",
    "fastify-static"
  ],
  "author": "zotijs",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.0.0",
    "@zotijstestingms/config-eslint": "workspace:*",
    "@zotijstestingms/config-typescript": "workspace:*",
    "eslint": "^9.8.0",
    "pino-pretty": "^11.2.2",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.3.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@zotijstestingms/common": "workspace:*",
    "fastify": "^4.28.1"
  }
